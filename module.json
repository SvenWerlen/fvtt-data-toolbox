{
  "title": "Data Toolbox for FoundryVTT",
  "description": "Tools for generating/manipulating data (compendiums). For example, it can be used for generating a compendium from a CSV file.",
  "url": "https://github.com/SvenWerlen/fvtt-data-toolbox",
  "version": "2.3.0",
  "scripts": [
    "data-toolbox.js",
    "scripts/d3/d3.min.js",
    "scripts/d3-fetch/d3-fetch.min.js"
  ],
  "styles": [
    "data-toolbox.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "fr",
      "name": "French",
      "path": "lang/fr.json",
      "flags": {}
    }
  ],
  "packs": [
    {
      "name": "data-macros",
      "label": "Data Macros",
      "path": "packs/data-macros.db",
      "private": false,
      "type": "Macro",
      "flags": {}
    }
  ],
  "manifest": "https://raw.githubusercontent.com/SvenWerlen/fvtt-data-toolbox/master/module.json",
  "download": "https://github.com/SvenWerlen/fvtt-data-toolbox/archive/v-2.3.0.zip",
  "id": "data-toolbox",
  "compatibility": {
    "minimum": "9",
    "verified": "10"
  }
}
